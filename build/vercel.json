{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://gloss-translation-api.vercel.app/api/:match*"
    },
    {
      "source": "/:match([^.]*)",
      "destination": "https://gloss-translation-web.vercel.app/index.html"
    },
    {
      "source": "/:match*",
      "destination": "https://gloss-translation-web.vercel.app/:match*"
    }
  ]
}